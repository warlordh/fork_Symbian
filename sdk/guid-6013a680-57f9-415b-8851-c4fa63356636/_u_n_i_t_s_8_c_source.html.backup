<a name="l00694"></a>00694                                 <span class="keywordflow">if</span> (!fgets(wantstr, 80, stdin)) {
<a name="l00695"></a>00695                                         <span class="keywordflow">if</span> (!quiet)
<a name="l00696"></a>00696                                                 putchar(<span class="charliteral">&#39;\n&#39;</span>);
<a name="l00697"></a>00697                                         exit(0);
<a name="l00698"></a>00698                                 }
<a name="l00699"></a>00699                         } <span class="keywordflow">while</span> (addunit(&amp;want, wantstr, 0) ||
