<a name="l00470"></a>00470         <span class="keywordflow">for</span> (i = 0; i &lt; prefixcount; i++) {
<a name="l00471"></a>00471                 <span class="keywordflow">if</span> (!strncmp(prefixtable[i].prefixname, unit,
<a name="l00472"></a>00472                         strlen(prefixtable[i].prefixname))) {
