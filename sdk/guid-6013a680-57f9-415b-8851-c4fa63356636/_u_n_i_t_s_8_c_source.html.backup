<a name="l00429"></a>00429         <span class="keywordtype">char</span> *copy;
<a name="l00430"></a>00430 
<a name="l00431"></a>00431         <span class="keywordflow">for</span> (i = 0; i &lt; unitcount; i++) {
<a name="l00432"></a>00432                 <span class="keywordflow">if</span> (!strcmp(unittable[i].uname, unit))
