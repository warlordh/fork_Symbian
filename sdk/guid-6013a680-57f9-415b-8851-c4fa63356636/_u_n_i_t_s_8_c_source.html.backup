<a name="l00599"></a>00599 
<a name="l00600"></a>00600 <span class="keywordtype">void</span> 
<a name="l00601"></a>00601 showanswer(<span class="keyword">struct</span> unittype * have, <span class="keyword">struct</span> unittype * want)
<a name="l00602"></a>00602 {
<a name="l00603"></a>00603         <span class="keywordflow">if</span> (compareunits(have, want)) {
<a name="l00604"></a>00604                 printf(<span class="stringliteral">&quot;conformability error\n&quot;</span>);
<a name="l00605"></a>00605                 showunit(have);
<a name="l00606"></a>00606                 showunit(want);
<a name="l00607"></a>00607         }
<a name="l00608"></a>00608         <span class="keywordflow">else</span>
<a name="l00609"></a>00609                 printf(<span class="stringliteral">&quot;\t* %.8g\n\t/ %.8g\n&quot;</span>, have-&gt;factor / want-&gt;factor,
<a name="l00610"></a>00610                     want-&gt;factor / have-&gt;factor);
<a name="l00611"></a>00611 }
