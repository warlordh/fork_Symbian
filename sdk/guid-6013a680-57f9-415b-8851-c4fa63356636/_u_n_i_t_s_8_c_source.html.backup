<a name="l00527"></a>00527                                 *product = NULLUNIT;
<a name="l00528"></a>00528                         }
<a name="l00529"></a>00529                         <span class="keywordflow">if</span> (addunit(theunit, toadd, flip))
<a name="l00530"></a>00530                                 <span class="keywordflow">return</span> ERROR;
<a name="l00531"></a>00531                 }
<a name="l00532"></a>00532         }
<a name="l00533"></a>00533         <span class="keywordflow">return</span> didsomething;
<a name="l00534"></a>00534 }
