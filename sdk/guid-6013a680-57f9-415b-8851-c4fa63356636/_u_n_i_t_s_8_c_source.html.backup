<a name="l00664"></a>00664                 strcpy(havestr, argv[optind]);
<a name="l00665"></a>00665                 strcpy(wantstr, argv[optind + 1]);
<a name="l00666"></a>00666                 initializeunit(&amp;have);
<a name="l00667"></a>00667                 addunit(&amp;have, havestr, 0);
<a name="l00668"></a>00668                 completereduce(&amp;have);
<a name="l00669"></a>00669                 initializeunit(&amp;want);
<a name="l00670"></a>00670                 addunit(&amp;want, wantstr, 0);
<a name="l00671"></a>00671                 completereduce(&amp;want);
<a name="l00672"></a>00672                 showanswer(&amp;have, &amp;want);
<a name="l00673"></a>00673         }
<a name="l00674"></a>00674         <span class="keywordflow">else</span> {
<a name="l00675"></a>00675                 <span class="keywordflow">if</span> (!quiet)
<a name="l00676"></a>00676                         printf(<span class="stringliteral">&quot;%d units, %d prefixes\n\n&quot;</span>, unitcount,
<a name="l00677"></a>00677                             prefixcount);
<a name="l00678"></a>00678                 <span class="keywordflow">for</span> (;;) {
<a name="l00679"></a>00679                         <span class="keywordflow">do</span> {
