<a name="l00458"></a>00458                 <span class="keywordflow">if</span> (copy[strlen(copy) - 1] == <span class="charliteral">&#39;e&#39;</span>) {
<a name="l00459"></a>00459                         copy[strlen(copy) - 1] = 0;
<a name="l00460"></a>00460                         <span class="keywordflow">for</span> (i = 0; i &lt; unitcount; i++) {
<a name="l00461"></a>00461                                 <span class="keywordflow">if</span> (!strcmp(unittable[i].uname, copy)) {
<a name="l00462"></a>00462                                         strcpy(buffer, copy);
