<a name="l00293"></a>00293         <span class="keywordflow">for</span> (slash = scratch + 1; *slash; slash++)
<a name="l00294"></a>00294                 <span class="keywordflow">if</span> (*slash == <span class="charliteral">&#39;-&#39;</span> &amp;&amp;
<a name="l00295"></a>00295                     (tolower(*(slash - 1)) != <span class="charliteral">&#39;e&#39;</span> ||
