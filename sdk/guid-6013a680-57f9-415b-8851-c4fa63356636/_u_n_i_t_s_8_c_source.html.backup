<a name="l00521"></a>00521                         }
<a name="l00522"></a>00522                         <span class="keywordflow">if</span> (strchr(toadd, PRIMITIVECHAR))
<a name="l00523"></a>00523                                 <span class="keywordflow">break</span>;
<a name="l00524"></a>00524                         didsomething = 1;
<a name="l00525"></a>00525                         <span class="keywordflow">if</span> (*product != NULLUNIT) {
