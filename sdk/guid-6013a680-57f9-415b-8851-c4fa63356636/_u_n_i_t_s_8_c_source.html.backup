<a name="l00579"></a>00579 
<a name="l00580"></a>00580 <span class="keywordtype">int</span> 
<a name="l00581"></a>00581 compareunits(<span class="keyword">struct</span> unittype * first, <span class="keyword">struct</span> unittype * second)
<a name="l00582"></a>00582 {
<a name="l00583"></a>00583         <span class="keywordflow">return</span>
<a name="l00584"></a>00584         compareproducts(first-&gt;numerator, second-&gt;numerator) ||
<a name="l00585"></a>00585         compareproducts(first-&gt;denominator, second-&gt;denominator);
<a name="l00586"></a>00586 }
<a name="l00587"></a>00587 
<a name="l00588"></a>00588 
<a name="l00589"></a>00589 <span class="keywordtype">int</span> 
<a name="l00590"></a>00590 completereduce(<span class="keyword">struct</span> unittype * unit)
<a name="l00591"></a>00591 {
<a name="l00592"></a>00592         <span class="keywordflow">if</span> (reduceunit(unit))
<a name="l00593"></a>00593                 <span class="keywordflow">return</span> 1;
<a name="l00594"></a>00594         sortunit(unit);
<a name="l00595"></a>00595         cancelunit(unit);
<a name="l00596"></a>00596         <span class="keywordflow">return</span> 0;
<a name="l00597"></a>00597 }
