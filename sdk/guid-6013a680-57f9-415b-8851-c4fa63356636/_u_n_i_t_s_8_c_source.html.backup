<a name="l00510"></a>00510                 product = theunit-&gt;numerator;
<a name="l00511"></a>00511 
<a name="l00512"></a>00512         <span class="keywordflow">for</span> (; *product; product++) {
<a name="l00513"></a>00513 
<a name="l00514"></a>00514                 <span class="keywordflow">for</span> (;;) {
<a name="l00515"></a>00515                         <span class="keywordflow">if</span> (!strlen(*product))
<a name="l00516"></a>00516                                 <span class="keywordflow">break</span>;
<a name="l00517"></a>00517                         toadd = lookupunit(*product);
<a name="l00518"></a>00518                         <span class="keywordflow">if</span> (!toadd) {
