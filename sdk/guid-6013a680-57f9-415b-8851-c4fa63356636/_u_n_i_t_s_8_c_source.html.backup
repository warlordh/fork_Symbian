<a name="l00324"></a>00324                                         }
<a name="l00325"></a>00325                                         <span class="keywordflow">if</span> (doingtop ^ flip)
<a name="l00326"></a>00326                                                 theunit-&gt;factor /= num;
<a name="l00327"></a>00327                                         <span class="keywordflow">else</span>
<a name="l00328"></a>00328                                                 theunit-&gt;factor *= num;
<a name="l00329"></a>00329                                 }
<a name="l00330"></a>00330                                 <span class="keywordflow">else</span> {
<a name="l00331"></a>00331                                         num = atof(item);
<a name="l00332"></a>00332                                         <span class="keywordflow">if</span> (!num) {
<a name="l00333"></a>00333                                                 zeroerror();
<a name="l00334"></a>00334                                                 <span class="keywordflow">return</span> 1;
<a name="l00335"></a>00335                                         }
<a name="l00336"></a>00336                                         <span class="keywordflow">if</span> (doingtop ^ flip)
<a name="l00337"></a>00337                                                 theunit-&gt;factor *= num;
<a name="l00338"></a>00338                                         <span class="keywordflow">else</span>
<a name="l00339"></a>00339                                                 theunit-&gt;factor /= num;
