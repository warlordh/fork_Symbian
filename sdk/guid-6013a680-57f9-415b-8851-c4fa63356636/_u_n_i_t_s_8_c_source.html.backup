<a name="l00687"></a>00687                                 }
<a name="l00688"></a>00688                         } <span class="keywordflow">while</span> (addunit(&amp;have, havestr, 0) ||
<a name="l00689"></a>00689                             completereduce(&amp;have));
<a name="l00690"></a>00690                         <span class="keywordflow">do</span> {
