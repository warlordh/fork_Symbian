<a name="l00078"></a>00078 <span class="comment">/*</span>
<a name="l00079"></a>00079 <span class="comment"> * Compute a POSIX 1003.2 checksum.  This routine has been broken out so that</span>
<a name="l00080"></a>00080 <span class="comment"> * other programs can use it.  It takes a file descriptor to read from and</span>
<a name="l00081"></a>00081 <span class="comment"> * locations to store the crc and the number of bytes read.  It returns 0 on</span>
<a name="l00082"></a>00082 <span class="comment"> * success and 1 on failure.  Errno is set on failure.</span>
<a name="l00083"></a>00083 <span class="comment"> */</span>
<a name="l00084"></a>00084 
<a name="l00085"></a>00085 
<a name="l00086"></a>00086 EXPORT_C <span class="keywordtype">int</span> crc(<span class="keyword">register</span> <span class="keywordtype">int</span> fd, u_long *cval, u_long *clen)
<a name="l00087"></a>00087 {
<a name="l00088"></a>00088         u_long crc_total = (u_long)~0;  <span class="comment">/* The crc over a number of files*/</span>
<a name="l00089"></a>00089         <span class="keyword">register</span> u_char *p;
<a name="l00090"></a>00090         <span class="keyword">register</span> <span class="keywordtype">int</span> nr;
<a name="l00091"></a>00091         <span class="keyword">register</span> u_long crc, len;
<a name="l00092"></a>00092 <span class="comment">//      u_char buf[16 * 1024]; // too large for stack</span>
<a name="l00093"></a>00093         u_char buf[3 * 1024];
<a name="l00094"></a>00094 
<a name="l00095"></a>00095 <span class="preprocessor">#define COMPUTE(var, ch)        (var) = (var) &lt;&lt; 8 ^ crctab[(var) &gt;&gt; 24 ^ (ch)]</span>
<a name="l00096"></a>00096 <span class="preprocessor"></span>
<a name="l00097"></a>00097         crc = len = 0;
<a name="l00098"></a>00098         crc_total = ~crc_total;
<a name="l00099"></a>00099         <span class="keywordflow">while</span> ((nr = read(fd, (<span class="keywordtype">char</span>*)buf, <span class="keyword">sizeof</span>(buf))) &gt; 0)
<a name="l00100"></a>00100                 <span class="keywordflow">for</span> (len += nr, p = buf; nr--; ++p) {
<a name="l00101"></a>00101                         COMPUTE(crc, *p);
<a name="l00102"></a>00102                         COMPUTE(crc_total, *p);
<a name="l00103"></a>00103                 }
<a name="l00104"></a>00104         <span class="keywordflow">if</span> (nr &lt; 0)
<a name="l00105"></a>00105                 <span class="keywordflow">return</span> (1);
<a name="l00106"></a>00106 
<a name="l00107"></a>00107         *clen = len;
<a name="l00108"></a>00108 
<a name="l00109"></a>00109         <span class="comment">/* Include the length of the file. */</span>
<a name="l00110"></a>00110         <span class="keywordflow">for</span> (; len != 0; len &gt;&gt;= 8) {
<a name="l00111"></a>00111                 COMPUTE(crc, len &amp; 0xff);
<a name="l00112"></a>00112                 COMPUTE(crc_total, len &amp; 0xff);
<a name="l00113"></a>00113         }
<a name="l00114"></a>00114 
<a name="l00115"></a>00115         *cval = ~crc;
<a name="l00116"></a>00116         crc_total = ~crc_total;
<a name="l00117"></a>00117         <span class="keywordflow">return</span> (0);
<a name="l00118"></a>00118 }
