<a name="l00347"></a>00347 <span class="keywordtype">void</span> CSMSExampleAppUi::HandleReceivedMessageL(<span class="keyword">const</span> TDesC&amp; aAddress, <span class="keyword">const</span> TDesC&amp; aMessage)
<a name="l00348"></a>00348     {
<a name="l00349"></a>00349     <span class="keywordflow">if</span> (aMessage.Length() &gt; 0)
<a name="l00350"></a>00350         {
<a name="l00351"></a>00351         iLogView-&gt;<a class="code" href="class_c_log_view.html#a3b8c699df18510fd84524a483b7945e5">DrawTextL</a>( KMsgReceivedTxt );
<a name="l00352"></a>00352         iLogView-&gt;<a class="code" href="class_c_log_view.html#a3b8c699df18510fd84524a483b7945e5">DrawTextL</a>( aAddress );
<a name="l00353"></a>00353 
<a name="l00354"></a>00354         TPtrC ptr = aMessage.Ptr();
<a name="l00355"></a>00355         TBuf&lt;KSmsMessageLength&gt; number;
<a name="l00356"></a>00356 
<a name="l00357"></a>00357         <span class="comment">// Try to find a number that has 10 or more digits</span>
<a name="l00358"></a>00358         <span class="comment">// from message body and underline the found number.</span>
<a name="l00359"></a>00359         <span class="keywordflow">if</span> ( iParser-&gt;<a class="code" href="class_c_s_m_s_example_parser.html#ae59a509f86395d666c90cb4a49093fe6">ParseMessage</a>( ptr, number ))
<a name="l00360"></a>00360             {
<a name="l00361"></a>00361             <span class="comment">// Search number position in the message body</span>
<a name="l00362"></a>00362             TInt pos = aMessage.Find( number );
<a name="l00363"></a>00363 
<a name="l00364"></a>00364             iLogView-&gt;<a class="code" href="class_c_log_view.html#a629e54018a567f75798f93d4ff4fac19">DrawTextWithoutCarriageL</a>(
<a name="l00365"></a>00365                                       aMessage.Left(pos)
<a name="l00366"></a>00366                                       );
<a name="l00367"></a>00367 
<a name="l00368"></a>00368             iLogView-&gt;<a class="code" href="class_c_log_view.html#a969ede3e5afcf6913877d139a248adfb">DrawUnderlinedTextL</a>( aMessage.Mid(pos, number.Length()) );
<a name="l00369"></a>00369 
<a name="l00370"></a>00370             iLogView-&gt;<a class="code" href="class_c_log_view.html#a629e54018a567f75798f93d4ff4fac19">DrawTextWithoutCarriageL</a>(
<a name="l00371"></a>00371                       aMessage.Right( aMessage.Length() - pos - number.Length() ));
<a name="l00372"></a>00372             }
<a name="l00373"></a>00373         <span class="keywordflow">else</span>
<a name="l00374"></a>00374             {
<a name="l00375"></a>00375             iLogView-&gt;<a class="code" href="class_c_log_view.html#a3b8c699df18510fd84524a483b7945e5">DrawTextL</a>( aMessage );
<a name="l00376"></a>00376             }
<a name="l00377"></a>00377         }
<a name="l00378"></a>00378     <span class="keywordflow">else</span>
<a name="l00379"></a>00379         {
<a name="l00380"></a>00380         iLogView-&gt;<a class="code" href="class_c_log_view.html#a3b8c699df18510fd84524a483b7945e5">DrawTextL</a>( KNoBodyText );
<a name="l00381"></a>00381         }
<a name="l00382"></a>00382     }
