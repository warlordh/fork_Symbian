<a name="l00074"></a>00074 <span class="comment">// Helper to convert signed 8-bit to signed 16-bit</span>
<a name="l00075"></a>00075 <span class="keywordtype">void</span> CMMFExPcm8Pcm16Codec::Convert(TUint8* aSrc, TUint8* aDst, TInt aSamples)
<a name="l00076"></a>00076         {
<a name="l00077"></a>00077         <span class="keyword">const</span> TInt KAndMask8bit = 0xff;
<a name="l00078"></a>00078         TInt s16;
<a name="l00079"></a>00079         <span class="keywordflow">while</span> (aSamples--)
<a name="l00080"></a>00080                 { 
<a name="l00081"></a>00081                 s16 = (*aSrc++)&lt;&lt;8;
<a name="l00082"></a>00082                 *aDst++ = STATIC_CAST( TInt8, s16&amp;KAndMask8bit);
<a name="l00083"></a>00083                 *aDst++ = STATIC_CAST( TInt8, (s16&gt;&gt;8)&amp;KAndMask8bit);
<a name="l00084"></a>00084                 }
<a name="l00085"></a>00085         }
