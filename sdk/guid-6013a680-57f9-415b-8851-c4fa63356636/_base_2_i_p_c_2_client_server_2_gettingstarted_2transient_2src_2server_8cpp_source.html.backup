<a name="l00061"></a>00061                 {
<a name="l00062"></a>00062                 TPtrC m(aMessage);
<a name="l00063"></a>00063                 <span class="keywordflow">if</span> (iReceiveLen&lt;aMessage.Length())
<a name="l00064"></a>00064                         m.Set(m.Left(iReceiveLen));
<a name="l00065"></a>00065                 TInt r=iReceiveMsg.Write(0,m);
<a name="l00066"></a>00066                 <span class="keywordflow">if</span> (r==KErrNone)
<a name="l00067"></a>00067                         iReceiveMsg.Complete(KErrNone);
<a name="l00068"></a>00068                 <span class="keywordflow">else</span>
<a name="l00069"></a>00069                         PanicClient(iReceiveMsg,EPanicBadDescriptor);
<a name="l00070"></a>00070                 }
